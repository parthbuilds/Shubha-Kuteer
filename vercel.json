{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/admin/(.*)",
            "dest": "/public/admin/$1"
        },
        {
            "src": "/user/(.*)",
            "dest": "/public/user/$1"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.js"
        }
    ]
}